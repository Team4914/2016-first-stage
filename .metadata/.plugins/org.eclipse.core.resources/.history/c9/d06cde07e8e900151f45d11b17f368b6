package org.usfirst.frc4914.WindsorBot.commands;

import edu.wpi.first.wpilibj.command.Command;
import org.usfirst.frc4914.WindsorBot.Robot;

/**
 *
 */
public class RotateCW extends Command {
	
	
	
	public void rotateCW(double angle) {    	
    	double initialBearing = gyro.getAngle();
     	double finalBearing = initialBearing + angle;
     	
     	initialBearing += 360;
     	initialBearing %= 360;
     	finalBearing += 360;
     	finalBearing %= 360;
     	
       	while (!(gyro.getAngle() > finalBearing - ANGLE_EPSILON && gyro.getAngle() < finalBearing + ANGLE_EPSILON)) {
 	      	setLeftVictor(1);
 	      	setRightVictor(-1);
       	}
       	
       	stop();
     }

    public RotateCW(double angle) {

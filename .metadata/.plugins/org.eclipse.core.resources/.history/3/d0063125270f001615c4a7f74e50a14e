    public OI() {
        // BEGIN AUTOGENERATED CODE, SOURCE=ROBOTBUILDER ID=CONSTRUCTORS

        codriverJoystick = new Joystick(1);
        
        codriverTrigger = new JoystickButton(codriverJoystick, 1);
        codriverTrigger.whenPressed(new ShootLow());
        driverJoystick = new Joystick(0);
        
        driverRightBumper = new JoystickButton(driverJoystick, 6);
        driverRightBumper.whileHeld(new Intake());


        // SmartDashboard Buttons
        SmartDashboard.putData("Autonomous Command", new AutonomousCommand());
        SmartDashboard.putData("Soft EStop", new SoftEStop());
        SmartDashboard.putData("Shoot Low", new ShootLow());
        SmartDashboard.putData("Shoot High", new ShootHigh());
        SmartDashboard.putData("Intake", new Intake());

